client
dev tun
proto udp
remote-random
remote la.west.usa.torguardvpnaccess.com 443
resolv-retry infinite
remote-cert-tls server
nobind
tun-mtu 1500
tun-mtu-extra 32
mssfix 1450
ca ca.crt
auth-user-pass /etc/openvpn/torguard/vpnpass.txt
comp-lzo
fast-io
ping-restart 0
route-delay 2
route-method exe
route-up /root/route-up.sh
down-pre down /root/route-down.sh
script-security 3 system
mute-replay-warnings
verb 3
